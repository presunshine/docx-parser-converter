{
  "name": "@omer-go/docx-parser-converter-ts",
  "private": false,
  "version": "0.0.2",
  "description": "A TypeScript library to convert DOCX files to WYSIWYG HTML or plain text formats while preserving styles.",
  "type": "module",
  "main": "./dist/docx-parser-converter.umd.js",
  "module": "./dist/docx-parser-converter.es.js",
  "types": "./dist/types/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/docx-parser-converter.es.js",
      "require": "./dist/docx-parser-converter.umd.js",
      "types": "./dist/types/main.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc --project tsconfig.json --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.ts' --ignore-pattern 'src/docx_parsers/**' --ignore-pattern 'src/docx_to_html/**' --ignore-pattern 'src/docx_to_txt/**' --ignore-pattern 'src/-legacy/**'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jszip": "^3.4.0",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.52.0",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.9.8",
    "jszip": "^3.10.1"
  },
  "keywords": [
    "docx",
    "converter",
    "html",
    "text",
    "parser",
    "typescript",
    "browser",
    "office",
    "word",
    "wysiwyg",
    "styles"
  ],
  "author": "Omer Hayun <omerha86@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omer-go/docx-parser-converter.git"
  },
  "bugs": {
    "url": "https://github.com/omer-go/docx-parser-converter/issues"
  },
  "homepage": "https://github.com/omer-go/docx-parser-converter"
}
